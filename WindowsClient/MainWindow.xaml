<Window x:Class="Overmind.ImageManager.WindowsClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:main="clr-namespace:Overmind.ImageManager.WindowsClient"
        mc:Ignorable="d" d:DataContext="{d:DesignInstance Type=main:MainViewModel}"
        Title="{Binding Path=ApplicationTitle}">
    <main:MainView DataContext="{Binding}"/>
</Window>
